	<parameter name="ACCOUNT_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT * FROM

(SELECT PA.SCHOOL_YEAR, TO_CHAR(CALENDAR_DATE, 'mon') MNTH, --Conversion de la date de l'absence à un mois
        ATTENDANCE_CODE,
        PKG_PROFILE_ÉLÈVE.GET_COURSE_COUNT(PA.SCHOOL_YEAR,  $P{ACCOUNT_ID} ,  --Cette fonction retourne le nombre de cours dans lequel
                   (case when (TO_DATE(PA.CALENDAR_DATE, 'DD/MM/YYYY') BETWEEN--     un élève est inscrit qui doit prendre l'assiduité.
                               TO_DATE(SS.SEMESTER_START, 'DD/MM/YYYY') AND  --Ce Case statement détermine, d'après la date de l'absence, 
                               TO_DATE(SS.SEMESTER_END, 'DD/MM/YYYY'))       --     le semestre courrant.
                         then SS.SEMESTER
                   end)) gcc
       
FROM PERIOD_ATTENDANCE PA, SCHOOL_SEMESTERS SS

WHERE PERSON_ID =  $P{ACCOUNT_ID} --Paramêtre dans Jaspersoft studio
AND PA.SCHOOL_CODE IN ('ESF', 'ESA', 'ESE')--On compte seulement les absences prises durant le séjour secondaire
AND SS.SCHOOL_CODE = PA.SCHOOL_CODE
AND SS.SCHOOL_YEAR = PA.SCHOOL_YEAR
AND CLASS_CODE IS NOT NULL --Dans le cas des absences secondaire, s'il y a une absence qui n'a pas de Class_Code associé, ce n'est pas une absence valide
AND ATTENDANCE_CODE NOT IN ('N', 'E', 'G') --On ne compte pas les absences "Justifié". "Jour avec pas d'enseignement" ou "Intempéries"
AND PA.SCHOOL_YEAR_TRACK = SS.SCHOOL_YEAR_TRACK
AND CALENDAR_DATE BETWEEN SS.SEMESTER_START AND SS.SEMESTER_END --Ceci est pour prévenir que le Case ci-dessus retourne des rangées vides
AND UPPER(SCHOOL_PERIOD) NOT IN ('DÎNER', 'DINER', 'ACCEUIL', 'ACC.', 'FOYER') --On ne compte pas les absences prises durant le dîner, ou pendant l'acceuil/foyer
) 


PIVOT (SUM(CASE WHEN ATTENDANCE_CODE = 'L' THEN 1 END) as L, 
       SUM(CASE WHEN ATTENDANCE_CODE = 'A' THEN round(1/nullif(gcc, 0), 2) END) as A --Le nullif est pour satisfaire Jaspersoft Studio qui se 
       FOR MNTH                                                                      --pleignait qu'il y avait une division par zéro. La division
       IN (                                                                          --détermine le poid de chaque absence. Par exemple, un élève 
                'sep',-- AS SEPTEMBRE,                                               --qui prend 4 cours aura un poid d'absence de 0,25. tandis 
                'oct',-- AS OCTOBRE,                                                 --que l'élève qui en prend 5 aura un poid de 0,2. Une absence
                'nov',-- AS NOVEMBRE,                                                --est égale à une journée complète de cours manqués. 
                'dec',-- AS DECEMBRE,
                'jan', -- AS JANVIER, 
                'feb',-- AS FÉVRIER, 
                'mar',-- AS MARS,
                'apr',-- AS AVRIL,
                'may',-- AS MAI, 
                'jun'-- AS JUIN
                
            ) -- END <IN>
) -- END PIVOT]]>
	</queryString>
