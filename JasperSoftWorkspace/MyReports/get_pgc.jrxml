	<style name="Style1" mode="Opaque" forecolor="#000000" backcolor="#FFFFFF"/>
	<parameter name="ACCOUNT_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT PREFERRED_SURNAME || ', ' || PREFERRED_FIRST_NAME as pgc_name, P.person_id, relationship_type_name_f,
STREET_NO || ' ' || STREET || chr(10) || JURISDICTION_NAME || ', ' || PROVINCE_STATE_CODE || chr(10) || POSTAL_CODE AS ADDRESS, 
       PKG_PROFILE_ÉLÈVE.get_PERSON_PHONE_HOME ( $P{ACCOUNT_ID} , SYSDATE ) as phone,
       PKG_PROFILE_ÉLÈVE.get_pgc_address_email ( $P{ACCOUNT_ID} , SYSDATE ) as email
FROM PERSONS P, STUDENT_CONTACTS SC, ADDRESSES A, PERSON_ADDRESSES PA, relationship_types rt
WHERE P.PERSON_ID = CONTACT_PERSON_ID
AND A.ADDRESS_ID = PA.ADDRESS_ID
AND PA.PERSON_ID = P.PERSON_ID
AND SYSDATE BETWEEN PA.START_DATE AND NVL(PA.END_DATE, SYSDATE+1)
AND STUDENT_PERSON_ID =  $P{ACCOUNT_ID} 
AND sysdate BETWEEN SC.START_DATE AND NVL(SC.END_DATE, SYSDATE+1)
and lower(access_to_records_flag) = 'x'
and address_type_name = 'Home'
and rt.relationship_type_name = sc.relationship_type_name]]>
	</queryString>
