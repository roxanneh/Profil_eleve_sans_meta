	<subDataset name="Dataset5" uuid="678ef505-1603-4755-93ac-52d53ac927c5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="ACCOUNT_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select regexp_replace(school_year, '(\w{4})(\w{4})', '\1-\2') school_year, school_code, trim(lower(mnth)) as mnth, calendar_date, attendance_code, a, l, person_id,

        case when trim(mnth) IN ('sep', 'sept.') then 0 
             when trim(mnth)  in ('oct', 'oct.') then 1 
             when trim(mnth)  in ('nov', 'nov.') then 2
             when trim(mnth)  in ('dec', 'déc.') then 3
             when trim(mnth)  in ('jan', 'janv.') then 4
             when trim(mnth)  in ('feb', 'févr.') then 5
             when trim(mnth)  in ('mar', 'mars') then 6
             when trim(mnth)  in ('apr', 'avr.') then 7
             when trim(mnth)  in ('may', 'mai') then 8
             when trim(mnth)  in ('jun', 'juin') then 9
             when trim(mnth)  in ('jul', 'juil.') then 10
             else 11
             end as NUM
             
from 
(select  SCHOOL_YEAR, SCHOOL_CODE, to_char(calendar_date, 'mon') MNTH, calendar_date, ATTENDANCE_CODE, person_id, 
    
--Nous faison un pivot sur les mois
--donc, un crosstab qui contient des colonnes de mois et des rangées par année scolaire
--Les absences sont divisés par le nombre de fois par jour que l'on prend les absences
--Par exemple, à la pluspart des écoles élémentaire, les profs prennent les absences
--2 fois par jour. Donc, une absence est égale à 1/2.
--De cette façon un élève manquera "une journée complête" lorsqu'il ou elle recoit 2 absences dans une journée
--Dans le cas de CDJ post-2012, les absences sont prises 6 fois par jours.  
           SUM(CASE WHEN ATTENDANCE_CODE = 'L' THEN 1 END) as L, 
           SUM(CASE WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G') 
                    AND SCHOOL_CODE NOT IN ('CDJ', 'EJE') 
                    THEN 1/2                  --ÉCOLES ÉLÉMENTAIRES QUI NE SONT PAS CDJ PRENNENT LES ABSENCES À CHAQUE (2) PÉRIODE
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G') 
                    AND SCHOOL_CODE IN ('CDJ') 
                    AND CALENDAR_DATE < TO_DATE('30/06/2012', 'DD/MM/YYYY') 
                    THEN 1/2                  --AVANT LE CHANGEMENT DES PÉRIODE À CDJ, EN 2012, CDJ PRENNAIT LES ABSENCES À CHAQUE (2) PÉRIODE 
                    
                       WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('CDJ')
                    AND CALENDAR_DATE > TO_DATE('30/06/2012', 'DD/MM/YYYY') 
                    THEN ROUND(1/6, 2)      --APRÈS LE CHANGEMENT DES PÉRIODES À CDJ, EN 2012, CDJ PRENNENT MAINTENT L'ABSENCE À CHAQUE (6) PÉRIODE
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('EJE')
                    AND CALENDAR_DATE BETWEEN TO_DATE('01/09/2004', 'DD/MM/YYYY') AND TO_DATE('30/06/2005', 'DD/MM/YYYY')
                    THEN 1/2
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('EJE')
                    AND CALENDAR_DATE BETWEEN TO_DATE('01/09/2005', 'DD/MM/YYYY') AND TO_DATE('30/06/2006', 'DD/MM/YYYY')
                    THEN 1/4
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('EJE')
                    AND CALENDAR_DATE BETWEEN TO_DATE('01/09/2006', 'DD/MM/YYYY') AND TO_DATE('30/06/2007', 'DD/MM/YYYY')
                    THEN ROUND(1/5, 2)
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('EJE')
                    AND CALENDAR_DATE BETWEEN TO_DATE('01/09/2007', 'DD/MM/YYYY') AND TO_DATE('30/06/2009', 'DD/MM/YYYY')
                    THEN ROUND(1/6, 2)
                    
                    WHEN ATTENDANCE_CODE IN ('A', 'N', 'E', 'G')
                    AND SCHOOL_CODE IN ('EJE')
                    AND CALENDAR_DATE BETWEEN TO_DATE('01/09/2009', 'DD/MM/YYYY') AND TO_DATE('30/06/2010', 'DD/MM/YYYY')
                    THEN 1/2
           END) as A
    
           
FROM PERIOD_ATTENDANCE

WHERE PERSON_ID =  $P{ACCOUNT_ID}  --Paramêtre dans Jaspersoft Studio

AND SCHOOL_CODE NOT IN ('ESF', 'ESA', 'ESE')  --On compte seulement les absences prises durant le séjour élémentaire
--AND ATTENDANCE_CODE NOT IN ('N', 'E', 'G') --On ne compte pas les absences "Justifié". "Jour avec pas d'enseignement" ou "Intempéries"
AND UPPER(SCHOOL_PERIOD) NOT IN ('DÎNER', 'DINER', 'ACCEUIL', 'ACC.') --On ne compte pas les absences prises durant le dîner, ou pendant l'acceuil/foyer

 group by SCHOOL_YEAR, 
        SCHOOL_CODE, 
        CALENDAR_DATE, 
        ATTENDANCE_CODE, person_id
        )]]>
		</queryString>
		<field name="SCHOOL_YEAR" class="java.lang.String"/>
		<field name="SCHOOL_CODE" class="java.lang.String"/>
		<field name="MNTH" class="java.lang.String"/>
		<field name="CALENDAR_DATE" class="java.sql.Timestamp"/>
		<field name="ATTENDANCE_CODE" class="java.lang.String"/>
		<field name="A" class="java.math.BigDecimal"/>
		<field name="L" class="java.math.BigDecimal"/>
		<field name="PERSON_ID" class="java.lang.String"/>
		<field name="NUM" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Dataset6" uuid="8b81ab49-b4ba-4c5b-b124-797b88fc8dc5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="ACCOUNT_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select regexp_replace(school_year, '(\w{4})(\w{4})', '\1-\2') school_year, MNTH, school_code, person_id,

case when trim(mnth) IN ('sep', 'sept.') then 0 
when trim(mnth)  in ('oct', 'oct.') then 1 
when trim(mnth)  in ('nov', 'nov.') then 2
when trim(mnth)  in ('dec', 'déc.') then 3
when trim(mnth)  in ('jan', 'janv.') then 4
when trim(mnth)  in ('feb', 'févr.') then 5
when trim(mnth)  in ('mar', 'mars') then 6
when trim(mnth)  in ('apr', 'avr.') then 7
when trim(mnth)  in ('may', 'mai') then 8
when trim(mnth)  in ('jun', 'juin') then 9
when trim(mnth)  in ('jul', 'juil.') then 10
else 11
end as NUM, 

attendance_code,

       SUM(CASE WHEN ATTENDANCE_CODE = 'L' THEN 1 END) as L, 
       SUM(CASE WHEN ATTENDANCE_CODE IN ('A', 'E', 'G', 'N')
                THEN 
                   round(1/gcc, 2)
                END) as A --Le nullif est pour satisfaire Jaspersoft Studio qui se 
                                                                                     --pleignait qu'il y avait une division par zéro. La division
                                                                                     --détermine le poid de chaque absence. Par exemple, un élève 
                                                                                     --qui prend 4 cours aura un poid d'absence de 0,25. tandis 
                                                                                     --que l'élève qui en prend 5 aura un poid de 0,2. Une absence
                                                                                     --est égale à une journée complète de cours manqués. 

from (
SELECT PA.SCHOOL_YEAR, TO_CHAR(CALENDAR_DATE, 'mon') MNTH, --Conversion de la date de l'absence à un mois
       ATTENDANCE_CODE, pa. school_code, person_id, 
       
       PKG_PROFILE_ÉLÈVE.GET_COURSE_COUNT(PA.SCHOOL_YEAR, $P{ACCOUNT_ID},  --Cette fonction retourne le nombre de cours dans lequel
                   (case when (PA.CALENDAR_DATE BETWEEN--     un élève est inscrit qui doit prendre l'assiduité.
                               SS.SEMESTER_START AND  --Ce Case statement détermine, d'après la date de l'absence, 
                               SS.SEMESTER_END)       --     le semestre courrant.
                         then SS.SEMESTER
                   end)) as gcc
                     
       
FROM PERIOD_ATTENDANCE PA, SCHOOL_SEMESTERS SS

WHERE PERSON_ID = $P{ACCOUNT_ID} 
AND PA.SCHOOL_CODE IN ('ESF', 'ESA', 'ESE')--On compte seulement les absences prises durant le séjour secondaire
AND SS.SCHOOL_CODE = PA.SCHOOL_CODE
AND SS.SCHOOL_YEAR = PA.SCHOOL_YEAR
AND CLASS_CODE IS NOT NULL --Dans le cas des absences secondaire, s'il y a une absence qui n'a pas de Class_Code associé, ce n'est pas une absence valide
--AND ATTENDANCE_CODE NOT IN ('N', 'E', 'G') --On ne compte pas les absences "Justifié". "Jour avec pas d'enseignement" ou "Intempéries"
AND PA.SCHOOL_YEAR_TRACK = SS.SCHOOL_YEAR_TRACK
AND CALENDAR_DATE BETWEEN SS.SEMESTER_START AND SS.SEMESTER_END --Ceci est pour prévenir que le Case ci-dessus retourne des rangées vides
AND UPPER(SCHOOL_PERIOD) NOT IN ('DÎNER', 'DINER', 'ACCUEIL', 'ACC.', 'FOYER') --On ne compte pas les absences prises durant le dîner, ou pendant l'acceuil/foyer
)
group by attendance_code, school_year, mnth, gcc, school_code, person_id
order by mnth asc]]>
		</queryString>
		<field name="SCHOOL_YEAR" class="java.lang.String"/>
		<field name="MNTH" class="java.lang.String"/>
		<field name="SCHOOL_CODE" class="java.lang.String"/>
		<field name="PERSON_ID" class="java.lang.String"/>
		<field name="NUM" class="java.math.BigDecimal"/>
		<field name="ATTENDANCE_CODE" class="java.lang.String"/>
		<field name="L" class="java.math.BigDecimal"/>
		<field name="A" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Dataset4" uuid="9eb3b85d-9e7d-41a5-be58-040c928e0328">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="ACCOUNT_ID" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT SORT_ORDER,
SC.REQUIREMENT_AREA,
REQ.DESCRIPTION_F,
NVL(REGEXP_REPLACE(SCHOOL_YEAR, '(\w{4})(\w{4})', '\1-\2'), '-') SCHOOL_YEAR,
NVL(PKG_PROFILE_ÉLÈVE.GET_EARNED_CREDIT(SC.PERSON_ID, SCC.SCHOOL_YEAR, SC.REQUIREMENT_AREA), 0) AS "EARNED CREDIT"
FROM  FS_REQUIREMENT_AREAS REQ
INNER JOIN FS_STUDENT_CREDITS SC
ON REQ.REQUIREMENT_AREA = SC.REQUIREMENT_AREA
LEFT JOIN FS_SECONDARY_COURSE_CREDIT SCC
ON SC.PERSON_ID = SCC.PERSON_ID
WHERE SC.PERSON_ID = $P{ACCOUNT_ID} 
AND SC.REQUIREMENT_AREA NOT IN ('21', '20')
ORDER BY SORT_ORDER]]>
		</queryString>
		<field name="SORT_ORDER" class="java.math.BigDecimal"/>
		<field name="REQUIREMENT_AREA" class="java.lang.String"/>
		<field name="DESCRIPTION_F" class="java.lang.String"/>
		<field name="SCHOOL_YEAR" class="java.lang.String"/>
		<field name="EARNED CREDIT" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="STUDENT" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT sr.OSR_FLAG, P.PREFERRED_NAME_UPPER, sr.school_year,
	   pkg_profile_élève.get_spoken_at_home(p.person_id) as spoken_at_home,
	   language_name_f, 
       P.LEGAL_FIRST_NAME,
       P.LEGAL_MIDDLE_NAME,
       P.LEGAL_SURNAME,
       STATUS_INDICATORS.STATUS_INDICATOR_CODE_F,
       P.GENDER,
       PKG_PROFILE_ÉLÈVE.GET_PNMI(p.person_id) AS PNMI,
       TO_CHAR(P.BIRTH_DATE, 'dd/mon/yyyy') as birth_date,
       EXTRACT(YEAR FROM SYSDATE) - EXTRACT(YEAR FROM P.BIRTH_DATE) - (
    CASE WHEN EXTRACT(MONTH FROM SYSDATE) = EXTRACT(MONTH FROM P.BIRTH_DATE) THEN (
        CASE WHEN EXTRACT(DAY FROM SYSDATE) < EXTRACT(DAY FROM P.BIRTH_DATE) THEN 1
        ELSE 0 END
    )
    WHEN EXTRACT(MONTH FROM SYSDATE) < EXTRACT(MONTH FROM P.BIRTH_DATE) THEN 1
    ELSE 0 END
) AS AGE_ANNEE,
CASE WHEN EXTRACT(MONTH FROM SYSDATE) > EXTRACT(MONTH FROM P.BIRTH_DATE)
    THEN (EXTRACT(MONTH FROM SYSDATE) - EXTRACT(MONTH FROM P.BIRTH_DATE))
    ELSE ((12 + EXTRACT(MONTH FROM SYSDATE)) - EXTRACT(MONTH FROM P.BIRTH_DATE))
END - CASE WHEN EXTRACT(DAY FROM SYSDATE) < EXTRACT(DAY FROM P.BIRTH_DATE) THEN 1 ELSE 0
END AS AGE_MOIS ,
       status_in_canada.FULL_NAME_F,
       PROGRAMS.PROGRAM_NAME_F,
       P.HEALTH_CARD_NO,
       TRUNC (MONTHS_BETWEEN (SYSDATE, P.BIRTH_DATE) / 12, 1) as age,
       VERIFICATION_DOCUMENTS.FULL_NAME_F,
       PKG_PROFILE_ÉLÈVE.GET_TEACHER_NAME (PKG_PROFILE_ÉLÈVE.FIND_MAIN_CLASS_TEACHER (
                                          sr.SCHOOL_CODE,
                                          PKG_PROFILE_ÉLÈVE.get_school_year (
                                             SYSDATE),
                                          p.person_id,
                                          SYSDATE)),
       P.STUDENT_NO,
       P.OEN_NUMBER,
       P.COHORT,
       SR.GRADE,
       PKG_PROFILE_ÉLÈVE.FIND_MAIN_CLASS_CODE (
          sr.SCHOOL_CODE,
          PKG_PROFILE_ÉLÈVE.get_school_year (SYSDATE),
          p.person_id,
          SYSDATE),
       PKG_PROFILE_ÉLÈVE.FIND_MAIN_CLASS_ROOM_NO (
          sr.SCHOOL_CODE,
          PKG_PROFILE_ÉLÈVE.get_school_year (SYSDATE),
          p.person_id,
          SYSDATE),
       SYSDATE,
       PKG_PROFILE_ÉLÈVE_TEST.get_pgc_address_home_1 (p.person_id, SYSDATE),
       PKG_PROFILE_ÉLÈVE_TEST.get_pgc_address_home_2 (p.person_id, SYSDATE),
       PKG_PROFILE_ÉLÈVE.get_pgc_address_mail (p.person_id, SYSDATE),
       regexp_replace(PKG_PROFILE_ÉLÈVE.get_PERSON_PHONE_HOME (p.person_id, SYSDATE ), '(\S{8})(\w{4})', '\1-\2') as phone,
       PKG_PROFILE_ÉLÈVE.get_pgc_address_email (p.person_id, SYSDATE),
       fs_doctors.surname,
       fs_doctors.first_name,
       fs_doctors.TELE_AREA_CODE,
       fs_doctors.TELE_NUMBER,
       fs_doctors.TELE_EXTENSION AS data,
       schools.school_name,
       p.person_id,
       p.total_community_hours,
       PKG_PROFILE_ÉLÈVE.GET_PEI(p.person_id) as PEI,
       PKG_PROFILE_ÉLÈVE.GET_ANOMALIE(p.person_id)as ANOMALIE,
       PKG_PROFILE_ÉLÈVE.GET_ALF(p.person_id) as ALF,
       PKG_PROFILE_ÉLÈVE.GET_YTD_ABS(p.person_id, sr.school_code, sr.school_year) as STU_ABS,
       PKG_PROFILE_ÉLÈVE.GET_YTD_LATES(p.person_id, PKG_PROFILE_ÉLÈVE.GET_SCHOOL_YEAR(sysdate), SYSDATE) as STU_LATES,
       (SELECT COUNT(*) FROM FS_SECONDARY_COURSE_CREDIT SCC WHERE SCC.PERSON_ID = P.PERSON_ID) AS CREDITS_SECONDAIRES
  FROM persons p,
       Student_registrations SR,
       student_programs,
       school_programs,
       programs,
       status_in_canada,
       STATUS_INDICATORS,
       languages l,
       VERIFICATION_DOCUMENTS,
       fs_doctors,
       schools
WHERE     p.person_id = sr.person_id
       AND p.status_in_canada_code =
              status_in_canada.status_in_canada_code(+)
       AND p.doctor_id = fs_doctors.doctor_id(+)
       AND p.language_of_legal_documents = l.LANGUAGE_NAME
       AND p.status_in_canada_verif_code =
              VERIFICATION_DOCUMENTS.VERIFICATION_DOCUMENT_CODE
       AND sr.person_id = student_programs.person_id
       AND sr.school_year = student_programs.school_year
       AND sr.school_code = student_programs.school_code
       AND sr.status_indicator_code = STATUS_INDICATORS.status_indicator_code
       AND sr.status_indicator_code IN ('Active', 'PreReg')
       AND sr.OSR_FLAG IN ('x')
       and sr.school_code = schools.school_Code
       AND P.PERSON_ID = $P{STUDENT}
       AND sr.grade NOT IN ('SK', 'JK')
       AND STUDENT_PROGRAMS.program_name = school_programs.program_name
       AND STUDENT_PROGRAMS.school_code = school_programs.school_code
       AND school_programs.program_name = programs.program_name
       AND sr.school_year = PKG_PROFILE_ÉLÈVE.get_school_year (SYSDATE)]]>
	</queryString>
