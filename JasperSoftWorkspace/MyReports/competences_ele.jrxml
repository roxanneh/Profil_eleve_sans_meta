	<parameter name="ACCOUNT_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[/********Select * from afin de pouvoir mettre les strands en ordre********/
select * from ((SELECT DISTINCT M_MEMBRE_ID, M_ECOLE, regexp_replace(M_ANNEESCOLAIRE, '(\w{4})(\w{4})', '\1-\2') M_ANNEESCOLAIRE, 

M_PERIODE_REF, GRADE, STRAND_DESCRIPTION_F,

PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, M_PERIODE_REF, M_SUBJECT_CODE, 1, M_STRAND_CODE, GRADE) AS MOY_CC,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, M_PERIODE_REF, M_SUBJECT_CODE, 2, M_STRAND_CODE, GRADE) AS MOY_HP,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, M_PERIODE_REF, M_SUBJECT_CODE, 3, M_STRAND_CODE, GRADE) AS MOY_COM,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, M_PERIODE_REF, M_SUBJECT_CODE, 4, M_STRAND_CODE, GRADE) AS MOY_MA


FROM ATELIER.SP_ELEVES_RESULTATS, STUDENT_REGISTRATIONS, STRAND, ATELIER.SP_EVALUATIONS

WHERE M_MEMBRE_ID =  $P{ACCOUNT_ID} 
AND PERSON_ID = M_MEMBRE_ID
AND M_ECOLE = SCHOOL_CODE
AND M_ANNEESCOLAIRE = SCHOOL_YEAR
AND M_STRAND_CODE = STRAND_CODE
AND M_STRAND_CODE IN ('001', '002', '003', '013', '015', '020', '021', '022', '023', '024', '')
AND M_ECOLE NOT IN ('ESA', 'ESF', 'ESE')
AND SCHOOL_CODE NOT IN ('ESA', 'ESF', 'ESE')
AND M_A_QUITTE != 'O' 

/****  Ne pas inclure les périodes des rapports qui n'ont pas encore d'évaluations *****/
AND E_ECOLE = M_ECOLE
AND E_ANNEESCOLAIRE = M_ANNEESCOLAIRE
AND E_CLASSE_CODE = M_CLASSE_CODE
AND E_CODECOURS = M_CODECOURS
AND E_PERIODE_REF = M_PERIODE_REF
AND E_SUBJECT_CODE = M_SUBJECT_CODE
AND E_STRAND_CODE = M_STRAND_CODE
AND E_STRAND_CODE IN ('001', '002', '003', '013', '015', '020', '021', '022', '023', '024', '')
)

/*******UNION SUR LA TABLES DES PÉRIODES 'P' POUR LE FRANÇAIS**********/
union

(select  M_MEMBRE_ID, M_ECOLE, regexp_replace(M_ANNEESCOLAIRE, '(\w{4})(\w{4})', '\1-\2') M_ANNEESCOLAIRE, CASE when M_periode_ref in ('I', 'F') then 'P' end as M_periode_ref, GRADE,
case when STRAND_DESCRIPTION_F IN ('Communication orale') 
                                OR STRAND_DESCRIPTION_F IN ('Écriture') 
                                OR STRAND_DESCRIPTION_F IN ('Lecture') 
                               THEN 'Français' end as STRAND_DESCRIPTION_F,

PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '009', 1) AS MOY_CC,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '009', 2) AS MOY_HP,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '009', 3) AS MOY_COM,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '009', 4) AS MOY_MA

FROM ATELIER.SP_ELEVES_RESULTATS, STUDENT_REGISTRATIONS, STRAND

WHERE M_MEMBRE_ID = $P{ACCOUNT_ID} 
AND PERSON_ID = M_MEMBRE_ID
AND M_ECOLE = SCHOOL_CODE
AND M_ANNEESCOLAIRE = SCHOOL_YEAR
AND M_STRAND_CODE = STRAND_CODE
AND M_STRAND_CODE IN ('001', '002', '003')
AND M_ECOLE NOT IN ('ESA', 'ESF', 'ESE')
AND SCHOOL_CODE NOT IN ('ESA', 'ESF', 'ESE')
AND M_A_QUITTE != 'O' 

/*******UNION SUR LA TABLES DES PÉRIODES 'P' POUR MATHS**********/
union

select DISTINCT  M_MEMBRE_ID, M_ECOLE, regexp_replace(M_ANNEESCOLAIRE, '(\w{4})(\w{4})', '\1-\2') M_ANNEESCOLAIRE, CASE when M_periode_ref in ('I', 'F') then 'P' end as M_periode_ref, GRADE,
case when STRAND_DESCRIPTION_F IN (q'[Géométrie et sens de l'espace]') 
                                OR STRAND_DESCRIPTION_F IN ('Mesure')  
                                OR STRAND_DESCRIPTION_F IN ('Modélisation et algèbre')
                                OR STRAND_DESCRIPTION_F IN ('Numération et sens du nombre')
                                OR STRAND_DESCRIPTION_F IN ('Traitement des données et probabilités')
                               THEN 'Mathématiques' end as STRAND_DESCRIPTION_F,

PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '9999000000030', 1) AS MOY_CC,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '9999000000030', 2) AS MOY_HP,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '9999000000030', 3) AS MOY_COM,
PKG_PROFILE_ÉLÈVE.GET_MOY_COMP_ELE_P(M_MEMBRE_ID, M_ECOLE, M_ANNEESCOLAIRE, '9999000000030', 4) AS MOY_MA

FROM ATELIER.SP_ELEVES_RESULTATS, STUDENT_REGISTRATIONS, STRAND

WHERE M_MEMBRE_ID = $P{ACCOUNT_ID} 
AND PERSON_ID = M_MEMBRE_ID
AND M_ECOLE = SCHOOL_CODE
AND M_ANNEESCOLAIRE = SCHOOL_YEAR
AND M_STRAND_CODE = STRAND_CODE
AND M_STRAND_CODE IN ('020', '021', '022', '023', '019')
AND M_ECOLE NOT IN ('ESA', 'ESF', 'ESE')
AND SCHOOL_CODE NOT IN ('ESA', 'ESF', 'ESE')
AND M_A_QUITTE != 'O' 
))
order by m_anneescolaire desc, case when STRAND_DESCRIPTION_F IN ('Français') then 0
                        when STRAND_DESCRIPTION_F IN ('Communication orale') then 1
                                                                    when STRAND_DESCRIPTION_F IN ('Écriture') then 2
                                                                    when STRAND_DESCRIPTION_F IN ('Lecture') then 3
                                                                    when STRAND_DESCRIPTION_F IN ('Mathématiques') then 4
                                                                    when STRAND_DESCRIPTION_F IN (q'[Géométrie et sens de l'espace]') then 5
                                                                    when STRAND_DESCRIPTION_F IN ('Mesure') then 6
                                                                    when STRAND_DESCRIPTION_F IN ('Modélisation et algèbre') then 7
                                                                    when STRAND_DESCRIPTION_F IN ('Numération et sens du nombre') then 8
                                                                    when STRAND_DESCRIPTION_F IN ('Traitement des données et probabilités') then 9
                                                                    else 8
                                                                end asc, M_PERIODE_REF desc]]>
	</queryString>
