AND SCSA.GRADE = CC.GRADE
AND CC.SUBJECT_CODE = S.SUBJECT_CODE
ORDER BY SCSA.SCHOOL_YEAR, SCSA.COURSE_CODE, SCSA.REPORT_PERIOD]]>
