	<subDataset name="Dataset15" uuid="ff9f848e-2cd1-46b1-bf72-2997758f819e">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="ACCOUNT_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select school_year, MNTH, school_code,

case when trim(mnth) IN ('sep', 'sept.') then 0 
when trim(mnth)  in ('oct', 'oct.') then 1 
when trim(mnth)  in ('nov', 'nov.') then 2
when trim(mnth)  in ('dec', 'déc.') then 3
when trim(mnth)  in ('jan', 'janv.') then 4
when trim(mnth)  in ('feb', 'févr.') then 5
when trim(mnth)  in ('mar', 'mars') then 6
when trim(mnth)  in ('apr', 'avr.') then 7
when trim(mnth)  in ('may', 'mai') then 8
when trim(mnth)  in ('jun', 'juin') then 9
when trim(mnth)  in ('jul', 'juil.') then 10
else 11
end as NUM, 

attendance_code,

       SUM(CASE WHEN ATTENDANCE_CODE = 'L' THEN 1 END) as L, 
       SUM(CASE WHEN ATTENDANCE_CODE IN ('A', 'E', 'G', 'N')
                THEN 
                   round(1/gcc, 2)
                END) as A --Le nullif est pour satisfaire Jaspersoft Studio qui se 
                                                                                     --pleignait qu'il y avait une division par zéro. La division
                                                                                     --détermine le poid de chaque absence. Par exemple, un élève 
                                                                                     --qui prend 4 cours aura un poid d'absence de 0,25. tandis 
                                                                                     --que l'élève qui en prend 5 aura un poid de 0,2. Une absence
                                                                                     --est égale à une journée complète de cours manqués. 

from (
SELECT PA.SCHOOL_YEAR, TO_CHAR(CALENDAR_DATE, 'mon') MNTH, --Conversion de la date de l'absence à un mois
       ATTENDANCE_CODE, pa. school_code, 
       
       PKG_PROFILE_ÉLÈVE.GET_COURSE_COUNT(PA.SCHOOL_YEAR,   $P{ACCOUNT_ID} ,  --Cette fonction retourne le nombre de cours dans lequel
                   (case when (PA.CALENDAR_DATE BETWEEN--     un élève est inscrit qui doit prendre l'assiduité.
                               SS.SEMESTER_START AND  --Ce Case statement détermine, d'après la date de l'absence, 
                               SS.SEMESTER_END)       --     le semestre courrant.
                         then SS.SEMESTER
                   end)) as gcc
                     
       
FROM PERIOD_ATTENDANCE PA, SCHOOL_SEMESTERS SS

WHERE PERSON_ID =  $P{ACCOUNT_ID}   --Paramêtre dans Jaspersoft studio
AND PA.SCHOOL_CODE IN ('ESF', 'ESA', 'ESE')--On compte seulement les absences prises durant le séjour secondaire
AND SS.SCHOOL_CODE = PA.SCHOOL_CODE
AND SS.SCHOOL_YEAR = PA.SCHOOL_YEAR
AND CLASS_CODE IS NOT NULL --Dans le cas des absences secondaire, s'il y a une absence qui n'a pas de Class_Code associé, ce n'est pas une absence valide
--AND ATTENDANCE_CODE NOT IN ('N', 'E', 'G') --On ne compte pas les absences "Justifié". "Jour avec pas d'enseignement" ou "Intempéries"
AND PA.SCHOOL_YEAR_TRACK = SS.SCHOOL_YEAR_TRACK
AND CALENDAR_DATE BETWEEN SS.SEMESTER_START AND SS.SEMESTER_END --Ceci est pour prévenir que le Case ci-dessus retourne des rangées vides
AND UPPER(SCHOOL_PERIOD) NOT IN ('DÎNER', 'DINER', 'ACCUEIL', 'ACC.', 'FOYER') --On ne compte pas les absences prises durant le dîner, ou pendant l'acceuil/foyer
)
group by attendance_code, school_year, mnth, gcc, school_code
order by mnth asc]]>
		</queryString>
		<field name="SCHOOL_YEAR" class="java.lang.String"/>
		<field name="MNTH" class="java.lang.String"/>
		<field name="SCHOOL_CODE" class="java.lang.String"/>
		<field name="NUM" class="java.math.BigDecimal"/>
		<field name="ATTENDANCE_CODE" class="java.lang.String"/>
		<field name="L" class="java.math.BigDecimal"/>
		<field name="A" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Dataset19" uuid="ec6f1e13-3844-4544-a412-53f5e7bdcbda">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="ACCOUNT_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT SCSA.SCHOOL_CODE, SCSA.SCHOOL_YEAR, SCSA.GRADE,  
       SCSA.REPORT_PERIOD, S.SUBJECT_DESCRIPTION_F, SCSA.MARK
       
FROM STUDENT_COURSE_STRAND_ACHIEVE SCSA, COURSE_CODES CC, SUBJECTS S

WHERE SCSA.PERSON_ID =  $P{ACCOUNT_ID} 
