	<parameter name="ACCOUNT_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SR.GRADE, P.PREFERRED_NAME_UPPER, P.GENDER, SCA.COURSE_CODE, SCA.COURSE_SECTION, CCS.CLASS_CODE,


PKG_PROFILE_ÉLÈVE.GET_ABS_COURS_SEC(SCA.PERSON_ID,SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SUBSTR(SCA.REPORT_PERIOD, 1,1), SCA.COURSE_CODE, SCA.COURSE_SECTION) as ABS/* YTD_ABS */,
PKG_PROFILE_ÉLÈVE.GET_RET_COURS_SEC(SCA.PERSON_ID,SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SUBSTR(SCA.REPORT_PERIOD, 1,1), SCA.COURSE_CODE, SCA.COURSE_SECTION) as RET/*YTD_LATES*/,

SUBSTR(SCA.REPORT_PERIOD,1,1) as SEMESTRE,
--SCA.REPORT_PERIOD as Semestre,
--PKG_PROFILE_ÉLÈVE.GET_NOTE_COURS_SEC(SCA.PERSON_ID,SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SCA.REPORT_PERIOD, SCA.COURSE_CODE, SCA.COURSE_SECTION) NOTE_M/*NOTE_M */
PKG_PROFILE_ÉLÈVE.GET_NOTE_COURS_SEC(SCA.PERSON_ID,SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SUBSTR(SCA.REPORT_PERIOD, 1,1)||'M', SCA.COURSE_CODE, SCA.COURSE_SECTION) NOTE_M,--NOTE_M,
PKG_PROFILE_ÉLÈVE.GET_NOTE_COURS_SEC(SCA.PERSON_ID,SCA.SCHOOL_CODE, SCA.SCHOOL_YEAR, SUBSTR(SCA.REPORT_PERIOD, 1,1)||'F', SCA.COURSE_CODE, SCA.COURSE_SECTION) NOTE_F --NOTE_F,

            FROM FS_SECONDARY_COURSE_ACHIEVE SCA, STUDENT_REGISTRATIONS SR, PERSONS P, COURSE_CODE_SECTION CCS
            WHERE P.PERSON_ID = SR.PERSON_ID
            AND SR.PERSON_ID = SCA.PERSON_ID
            AND P.PERSON_ID = $P{ACCOUNT_ID}
            AND SR.SCHOOL_YEAR = SCA.SCHOOL_YEAR
            AND SR.SCHOOL_CODE = SCA.SCHOOL_CODE
            AND SR.SCHOOL_CODE IN ('ESE', 'ESA', 'ESF')
            /*AND (SR.STATUS_INDICATOR_CODE = 'Active'
            or sr.person_id in --ceci à été ajouté le 2 juillet 2013 pour inclure les élèves qui on eu un départ la dernier journée scolaire 
            (select person_id from student_enrolments se where se.transaction_type_ind = '5' and se.effective_date = to_date('2015-06-26','YYYY-MM-DD'))) */
          --    AND SR.SCHOOL_YEAR IN ('20142015', '20132014')
            AND SCA.SCHOOL_CODE = CCS.SCHOOL_CODE
            AND SCA.SCHOOL_YEAR = CCS.SCHOOL_YEAR
            AND SCA.COURSE_CODE = CCS.COURSE_CODE
            AND SCA.COURSE_SECTION = CCS.COURSE_SECTION
            AND CCS.BUILDER_SET_CODE = 'working'
            AND SCA.REPORT_PERIOD IN ('1'||'M','1'||'F','2'||'M','2'||'F') 
            /*AND (SCA.COURSE_CODE LIKE('FRA1D%') OR 
            SCA.COURSE_CODE LIKE('FRA1P%') OR 
            SCA.COURSE_CODE LIKE('FRA1L%') OR 
            SCA.COURSE_CODE LIKE('FRA2P%') OR 
            SCA.COURSE_CODE LIKE('FRA2D%') OR 
            SCA.COURSE_CODE LIKE('FRA2L%') OR 
            SCA.COURSE_CODE LIKE('MPM1D%') OR 
            SCA.COURSE_CODE LIKE('MFM1P%') OR 
            SCA.COURSE_CODE LIKE('MAT1L%') OR 
            SCA.COURSE_CODE LIKE('MPM2D%') OR 
            SCA.COURSE_CODE LIKE('MFM2P%') OR 
            SCA.COURSE_CODE LIKE('MAT2L%'))*/
            order by SCA.SCHOOL_YEAR desc, grade asc, SEMESTRE asc, class_code]]>
	</queryString>
