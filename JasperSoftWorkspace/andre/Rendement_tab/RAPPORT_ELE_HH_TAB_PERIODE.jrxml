	<subDataset name="Dataset1" uuid="7578291a-6833-4ea8-a04a-cddef66f7382">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="SCHOOL_YEAR" class="java.lang.String" isForPrompting="false">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="SCHOOL_CODE" class="java.util.List" isForPrompting="false"/>
		<parameter name="GRADE" class="java.util.List" isForPrompting="false">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="CLASS_CODE" class="java.util.List" isForPrompting="false"/>
		<parameter name="REPORT_PERIOD" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[select * from (SELECT DISTINCT SR.PERSON_ID, P.LEGAL_NAME_UPPER,
SR.SCHOOL_CODE,
to_number(SR.GRADE) as grade,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_CLASS_CODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, SR.GRADE) AS CLASS_CODE,
P.GENDER,
SR.status_indicator_code,
case when besoins = 1 then 'Très grand besoin'
            when besoins = 2 then 'Grand besoin'
            when besoins = 3 then 'Besoin modéré'
            when besoins = 4 then 'Besoin léger'
            else 'N/A' end as "Niveau de besoin",
      
       case when programme_matiere = 1 then 'ALF' 
            when programme_matiere = 2 then 'Français'
            else 'N/A' end as "Programme",
            
nvl(REPLACE(PKG_RAPPORT_DE_RENDEMENT_ELE.GET_ABS_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{REPORT_PERIOD}), ',', '.'), 0) AS YTD_ABS,
nvl(PKG_RAPPORT_DE_RENDEMENT_ELE.GET_RET_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{REPORT_PERIOD}), 0) AS YTD_LATES,
case when PKG_RAPPORT_DE_RENDEMENT_ELE.GET_REPORT_PEI_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{REPORT_PERIOD}) = 'x' then 'Oui'
else 'Non' end AS PEI,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_REPORT_ANOMALIE_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{REPORT_PERIOD}) AS ANOMALIE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_ALS_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,  $P{REPORT_PERIOD}) AS ALS,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_NATIVE_FLAG(SR.PERSON_ID) AS PNMI,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_USE_OF_ORAL_FRE') AS FRANCAIS_ORAL,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_RESPONSIBILITY') AS FIABILITE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_ORGANIZATION') AS ORGANISATION,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_INDEPENDENT_WORK') AS AUTONOMIE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_COLLABORATION') AS COLLABORATION,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_INITIATIVE') AS INITIATIVE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,$P{REPORT_PERIOD}, 'LS_SELF_REGULATION') AS AUTOREGULATION,

GET_SCHOOL_YEAR_START(sr.school_year, sr.school_code) as school_year_start
FROM STUDENT_REGISTRATIONS SR
INNER JOIN PERSONS P
ON P.PERSON_ID = SR.PERSON_ID
left join 
trill02.student_alf_pana sap
on
sap.school_code = sr.school_code
and sap.school_year = sr.school_year
and sap.person_id = sr.person_id

WHERE  SR.SCHOOL_YEAR = $P{SCHOOL_YEAR}
AND $X{IN, SR.SCHOOL_CODE, SCHOOL_CODE}
AND $X{IN, SR.GRADE, GRADE}
AND SR.OSR_FLAG = 'x'
AND SR.STATUS_INDICATOR_CODE IN ('Active', 'Inactive')
and ((SAP.DATE_EN_VIGUEUR = 
        (SELECT MAX (DATE_EN_VIGUEUR) FROM trill02.student_alf_pana SAP 
            WHERE SAP.PERSON_ID = SR.PERSON_ID 
            and sap.school_year = sr.school_year 
            and sap.school_code = sr.school_code
            AND SAP.DATE_EN_VIGUEUR BETWEEN 
                PKG_RAPPORT_DE_RENDEMENT_ELE.GET_PERIOD_START(sr.school_code, sr.school_year, $P{REPORT_PERIOD}) 
                AND 
                PKG_RAPPORT_DE_RENDEMENT_ELE.GET_PERIOD_END(sr.school_code, sr.school_year, $P{REPORT_PERIOD}))) 
     or SAP.DATE_EN_VIGUEUR is null)
     
ORDER BY Sr.SCHOOL_CODE ASC, SR.GRADE ASC, P.LEGAL_NAME_UPPER ASC)
where $X{IN, class_code, CLASS_CODE}]]>
		</queryString>
