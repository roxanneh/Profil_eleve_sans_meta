	<subDataset name="Dataset1" uuid="b1f9426b-e708-4d92-b4d3-984461306761">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="CSCFN TRIL"/>
		<parameter name="SCHOOL_YEAR" class="java.lang.String" isForPrompting="false">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="SCHOOL_CODE" class="java.util.List" isForPrompting="false"/>
		<parameter name="GRADE" class="java.util.List" isForPrompting="false">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="CLASS_CODE" class="java.util.List" isForPrompting="false"/>
		<parameter name="PERIODE" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[select * from (SELECT DISTINCT S.SCHOOL_NAME, sr.school_year,
SR.SCHOOL_CODE,
to_number(SR.GRADE) as grade,
SR.PERSON_ID,
P.LEGAL_NAME_UPPER,
P.GENDER,
SR.STATUS_INDICATOR_CODE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_CLASS_CODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, SR.GRADE, '009') as class_code,
nvl(REPLACE(PKG_RAPPORT_DE_RENDEMENT_ELE.GET_ABS_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}), ',', '.'), 0) AS YTD_ABS,
nvl(PKG_RAPPORT_DE_RENDEMENT_ELE.GET_RET_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}), 0) AS YTD_LATES,
case when PKG_RAPPORT_DE_RENDEMENT_ELE.GET_REPORT_PEI_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}) = 'x' then 'Oui'
else 'Non' end AS PEI,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_REPORT_ANOMALIE_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}) AS ANOMALIE,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_ALS_PERIODE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR,  $P{PERIODE}) AS ALS,
case when besoins = 1 then 'Très grand besoin'
            when besoins = 2 then 'Grand besoin'
            when besoins = 3 then 'Besoin modéré'
            when besoins = 4 then 'Besoin léger'
            else 'N/A' end as "Niveau de besoin",
      
       case when programme_matiere = 1 then 'ALF' 
            when programme_matiere = 2 then 'Français'
            else 'N/A' end as "Programme",
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_NATIVE_FLAG(SR.PERSON_ID) AS PNMI,

pkg_profile_élève.oqre_get_cote_rwm('3', 'r', SR.PERSON_ID) as r3,
pkg_profile_élève.oqre_get_cote_rwm('3', 'w', SR.PERSON_ID) as w3,
pkg_profile_élève.oqre_get_cote_rwm('6', 'r', SR.PERSON_ID) as r6,
pkg_profile_élève.oqre_get_cote_rwm('6', 'w', SR.PERSON_ID) as w6,

PKG_RAPPORT_DE_RENDEMENT_ELE.GET_NOTE_ELE(SR.PERSON_ID, SR.SCHOOL_YEAR, $P{PERIODE},'003') AS COM,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_NOTE_ELE(SR.PERSON_ID, SR.SCHOOL_YEAR, $P{PERIODE},'002') AS ECR,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_NOTE_ELE(SR.PERSON_ID, SR.SCHOOL_YEAR, $P{PERIODE}, '001') AS LEC,

PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '1', '003', SR.GRADE) AS COMP_COM_CC,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '2', '003', SR.GRADE) AS COMP_COM_HP,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '3', '003', SR.GRADE) AS COMP_COM_COM,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '4', '003', SR.GRADE) AS COMP_COM_MA,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '1', '002', SR.GRADE) AS COMP_ECR_CC,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '2', '002', SR.GRADE) AS COMP_ECR_HP,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '3', '002', SR.GRADE) AS COMP_ECR_COM,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '4', '002', SR.GRADE) AS COMP_ECR_MA,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '1', '001', SR.GRADE) AS COMP_LEC_CC,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '2', '001', SR.GRADE) AS COMP_LEC_HP,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '3', '001', SR.GRADE) AS COMP_LEC_COM,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_MOY_COMP_ELE(SR.PERSON_ID, SR.SCHOOL_CODE, SR.SCHOOL_YEAR, $P{PERIODE}, '009', '4', '001', SR.GRADE) AS COMP_LEC_MA,
PKG_RAPPORT_DE_RENDEMENT_ELE.GET_COTE_HH_ELE(sap.PERSON_ID, sap.SCHOOL_CODE, sap.SCHOOL_YEAR,$P{PERIODE}, 'LS_USE_OF_ORAL_FRE') AS FRANCAIS_ORAL,

GET_SCHOOL_YEAR_START(sr.school_year, sr.school_code) as school_year_start

FROM STUDENT_REGISTRATIONS SR
INNER JOIN PERSONS P
ON P.PERSON_ID = SR.PERSON_ID
INNER JOIN SCHOOLS S
ON SR.SCHOOL_CODE = S.SCHOOL_CODE
left join 
trill02.student_alf_pana sap
on
sap.school_code = sr.school_code
and sap.school_year = sr.school_year
and sap.person_id = sr.person_id

WHERE  SR.SCHOOL_YEAR = $P{SCHOOL_YEAR}
AND $X{IN, SR.SCHOOL_CODE, SCHOOL_CODE}
AND $X{IN, SR.GRADE, GRADE}
AND SR.OSR_FLAG = 'x'
AND SR.STATUS_INDICATOR_CODE IN ('Active', 'Inactive')



ORDER BY S.SCHOOL_NAME ASC, GRADE ASC, P.LEGAL_NAME_UPPER ASC)
where $X{IN, class_code, CLASS_CODE}]]>
		</queryString>
