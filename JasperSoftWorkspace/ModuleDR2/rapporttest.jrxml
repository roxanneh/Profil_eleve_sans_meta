	<queryString>
		<![CDATA[select cat_name, c.cat_id,
       count(case when c.CAT_ID = 1 then 1 end) as Français_oral,
       count(case when c.CAT_ID = 2 then 1 end) as Fiabilité,
       count(case when c.CAT_ID = 3 then 1 end) as Organisation,
       count(case when c.CAT_ID = 4 then 1 end) as Autonomie,
       count(case when c.CAT_ID = 5 then 1 end) as Collaboration,
       count(case when c.CAT_ID = 6 then 1 end) as Initiative,
       count(case when c.CAT_ID = 7 then 1 end) as Autorégulation

from dr2_questions_reponse qr, dr2_categories c, dr2_questions q

where qr.question_id = q.Question_ID
and q.Cat_ID = c.CAT_ID


group by cat_name, c.cat_id]]>
	</queryString>
